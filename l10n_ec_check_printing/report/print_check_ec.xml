<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>

<template id="print_check_ec">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="o">
            <div t-att-data-oe-model="o._name" 
                 t-att-data-oe-id="o.id" 
                 t-foreach="o._check_get_pages()" 
                 t-as="page" 
                 t-attf-class="page article ckec_page {{ o.journal_id.l10n_ec_bank_check_format }}" 
                 t-attf-style="top: {{ o.journal_id.l10n_ec_check_margin_top }}in; left: {{ o.journal_id.l10n_ec_check_margin_left }}in;">
                <t t-call="l10n_ec_check_printing.ckec_check" />
            </div>
        </t>
    </t>
</template>

</data>
</odoo>
